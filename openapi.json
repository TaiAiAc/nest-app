        "responses": {
          "201": {
            "description": "搜索成功",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          }
        },
        "summary": "条件搜索用户",
        "tags": [
          "用户"
        ]
      }
    },
