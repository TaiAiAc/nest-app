  "devDependencies": {
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.9",
    "@types/express": "^5.0.0",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.2",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
